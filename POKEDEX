public class POKEDEX {
    public Pokemon next;
    public Pokemon first;
    public int count;
    public String [] Type = {"Normal", "Fire", "Water", "Grass", "Electric", "Ice", "Fighting", "Poison", "Ground", "Flying", "Psychic", "Bug", "Rock", "Ghost", "Dragon", "Dark", "Seel", "Fairy"}
    
    public void AddPokemon (String n, int s,int a, int sa, int sd, int d, int t1, int t2, boolean st) { 
        next = new Pokemon (n, s, a, sa, sd, d, t1, t2, st);
        count ++;
    }
    
        
    public String giveTypeName(int t){
        return Type[t];
    }
}
