package pokedexSystem;

public class Main {			//Hauptklasse des Codes, wird autmatisch ausgef√ºhrt und startet die methoden der anderen Klassen
	public static POKEDEX Dex;
    public static Pokemon POk;
    public static NODE node;
	public static saving Save;
	public static UserInterface UI;

    
	public static void main(String[] args) {
		POk = new Pokemon (1, "Bulbasaur", 45, 49, 49, 65, 65, 45, 3, 7, true);
		node = new NODE (null, POk);
		Dex = new POKEDEX(node);
		Save = new saving(Dex);
		saving.read();
		Dex.printAll();
		UI = new UserInterface(Dex);
	}

}
